{
    "name": "d3f3kt/oro-basics-bundle",
    "description": "Basic configuration, helper and tools for an oro application",
    "homepage": "https://github.com/RocKordier/OroBasicsBundle",
    "keywords": ["Oro Platform", "Symfony2", "Basics"],
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "[RocKordier] Eric Hertwig",
            "homepage": "http://eric-hertwig.de"
        }
    ],
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "autoload": {
        "psr-4": {
            "EHDev\\BasicsBundle\\": ["src/", "tests/"]
        }
    },
    "require": {
        "php": ">=7.0",
        "oro/platform": "^2.3 | ^2.6 | ^3.1",
        "juanparati/emoji": "@dev",
        "gedmo/doctrine-extensions": "^2.4",
        "stof/doctrine-extensions-bundle": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "ehdev/ciutility": "^0.0.1"
    },
    "config" : {
        "fxp-asset" : {
            "enabled" : false
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
